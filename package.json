{
    "name": "sunshinec-code-snippets",
    "displayName": "VSCode Snippets",
    "description": "SunshineC 的开发常用代码片段",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Snippets"
    ],
    "icon": "images/icon.jpg",
    "repository": {
        "type": "git",
        "url": "https://gitee.com/SunshineC/vscode-snippet"
    },
    "contributes": {
        "snippets": [
            {
                "language": "vue",
                "path": "./snippets/vue.json"
